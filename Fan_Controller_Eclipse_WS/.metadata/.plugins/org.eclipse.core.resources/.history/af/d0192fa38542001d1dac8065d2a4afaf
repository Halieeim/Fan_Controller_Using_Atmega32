#ifndef DCMOTOR_H_
#define DCMOTOR_H_

typedef enum{stop,CW,A_CW}DcMotor_State;

void DcMotor_Init(void);
void DcMotor_Rotate(DcMotor_State state,uint8 speed);



#endif /* DCMOTOR_H_ */
